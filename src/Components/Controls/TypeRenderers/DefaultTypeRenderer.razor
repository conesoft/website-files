@using Conesoft.Website.Files.Components.Controls.FileRenderers

@code {
    // [Parameter] public Conesoft.Files.File Current { get; set; } = default!;
    [Parameter] public Conesoft.Files.Entry Current { get; set; } = default!;
    [Parameter] public string Path { get; set; } = "";

    [Obsolete]
    public virtual string[] Extensions => [];

    public virtual string? Name => this.GetType().Name.Replace("TypeRenderer", "").Replace("Default", "File");

    public virtual IComparer<Conesoft.Files.Entry> Comparer => Comparer<Conesoft.Files.Entry>.Create((a, b) => naturalSortComparer.Compare(a.Name, b.Name));

    public virtual bool InCategory(Conesoft.Files.Entry entry) => true;

    protected static Conesoft.Tools.NaturalSortComparer naturalSortComparer = new Conesoft.Tools.NaturalSortComparer();
}
<DefaultFileRenderer Path="@Path" Current="@Current"></DefaultFileRenderer>